    "build:webpack:windows": "node_modules\\.bin\\webpack",
    "build:webpack:windows:production": "node_modules\\.bin\\webpack -p",
    "devServer": "./node_modules/.bin/webpack-dev-server --progress --colors",
    "compile": "sass --watch src/scss:dist/css",
    "build:production": "gulp",
    "test": "node_modules/.bin/eslint src/js/*.js --cache --cache-location ./.eslintcache",
    "test:quick-fix": "node_modules/.bin/eslint src/js/*.js --cache --cache-location ./.eslintcache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohseenrm/second_website.git"
  },
  "keywords": [
