    "gulp-minify-css": "^1.2.4",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^2.0.0",
    "gulp-uglifycss": "^1.0.8",
    "gulp-util": "^3.0.8",
    "postcss-loader": "^2.0.6",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.1.11"
